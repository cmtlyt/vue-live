{
  "name": "@vlive/runtime-core",
  "version": "0.0.1",
  "description": "浏览器运行时",
  "main": "dist/runtime-core.cjs.js",
  "module": "dist/runtime-core.esm.js",
  "files": [
    "dist"
  ],
  "sideEffect": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmtlyt/vue-live.git",
